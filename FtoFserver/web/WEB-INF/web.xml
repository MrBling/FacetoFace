<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>addRecord</servlet-name>
        <servlet-class>addRecord.addRecord</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addRecord</servlet-name>
        <url-pattern>/FtoFserver/addRecord</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Attendance</servlet-name>
        <servlet-class>Refer.Attendance</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Attendance</servlet-name>
        <url-pattern>/FtoFserver/Attendance</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>NotAttend</servlet-name>
        <servlet-class>Refer.NotAttend</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NotAttend</servlet-name>
        <url-pattern>/FtoFserver/NotAttend</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Refercount</servlet-name>
        <servlet-class>Refer.Refercount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Refercount</servlet-name>
        <url-pattern>/FtoFserver/Refercount</url-pattern>
    </servlet-mapping>

</web-app>

